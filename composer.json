{
    "name": "robuust/craft-commerce-pay",
    "description": "PAY integration for Craft Commerce 4.0+",
    "version": "1.1.4",
    "type": "craft-plugin",
    "keywords": [
        "pay",
        "commerce",
        "craftcms",
        "omnipay",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robuust",
            "homepage": "https://robuust.digital/"
        }
    ],
    "support": {
        "email": "support@robuust.digital",
        "issues": "https://github.com/robuust/craft-commerce-pay/issues?state=open",
        "source": "https://github.com/robuust/craft-commerce-pay",
        "docs": "https://github.com/robuust/craft-commerce-pay",
        "rss": "https://github.com/robuust/craft-commerce-pay/commits/master.atom"
    },
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "craftcms/commerce": "^4.0.0",
        "craftcms/commerce-omnipay": "^4.0.0",
        "paynl/omnipay-paynl": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\pay\\": "src/"
        }
    },
    "extra": {
        "name": "PAY for Craft Commerce",
        "handle": "commerce-pay"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/phpstan": "dev-main",
        "craftcms/ecs": "dev-main"
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G",
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        },
        "platform": {
            "php": "8.0.2"
        }
    }
}
